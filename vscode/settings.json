{
    "workbench.iconTheme": "vs-minimal",
    "workbench.colorTheme": "Atom One Dark",
    "window.zoomLevel": 0,
    "editor.fontSize": 13,
    "editor.scrollBeyondLastLine": false,
    "files.insertFinalNewline": true,
    "terminal.integrated.fontSize": 13,
    "files.trimTrailingWhitespace": true,
    "editor.tabSize": 2,
    // "workbench.useExperimentalGridLayout": true,
    "files.eol": "\n",
    "explorer.openEditors.visible": 0,
    "explorer.confirmDragAndDrop": false,
    "editor.minimap.enabled": false,
    "files.exclude": {
        "**/node_modules": true
    },
    "emmet.showAbbreviationSuggestions": false,
    "emmet.showExpandedAbbreviation": "never",
    "editor.codeActionsOnSave": {
        "source.fixAll": true,
        "source.organizeImports": true,
    },
    "terminal.external.osxExec": "Hyper.app",
    "terminal.integrated.fontFamily": "Meslo LG S for Powerline",
    "terminal.integrated.letterSpacing": 1,
    "vs-kubernetes": {
        "vs-kubernetes.helm-path": "/Users/teunvanvegchel/.vs-kubernetes/tools/helm/darwin-amd64/helm",
        "vs-kubernetes.minikube-path": "/Users/teunvanvegchel/.vs-kubernetes/tools/minikube/darwin-amd64/minikube",
        "vs-kubernetes.draft-path": "/Users/teunvanvegchel/.vs-kubernetes/tools/draft/darwin-amd64/draft"
    },
    "breadcrumbs.enabled": false,
    "search.useIgnoreFiles": false,
    "editor.gotoLocation.multiple": "goto",
    "references.preferredLocation": "view",
    "workbench.startupEditor": "newUntitledFile"
}
